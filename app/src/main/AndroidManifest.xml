<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="akov.example.greattrack"
    android:versionCode="4">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="akov.example.greattrack.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="akov.example.greattrack.habit.HabitDialogueActivity"
            android:parentActivityName="akov.example.greattrack.MainActivity">
        </activity>
        <activity android:name="akov.example.greattrack.habit.HabitLog"
            android:parentActivityName="akov.example.greattrack.MainActivity">
        </activity>
        <activity android:name="akov.example.greattrack.habit.HabitStats"
            android:parentActivityName="akov.example.greattrack.MainActivity">
        </activity>
        <activity android:name="akov.example.greattrack.habit.editHabit"
            android:parentActivityName="akov.example.greattrack.MainActivity">
        </activity>
        <activity
            android:name="akov.example.greattrack.budget.createBudget"
            android:parentActivityName="akov.example.greattrack.MainActivity" />
        <activity
            android:name="akov.example.greattrack.budget.editBudget"
            android:parentActivityName="akov.example.greattrack.MainActivity" />
        <activity android:name="akov.example.greattrack.budget.budgetLedger"
            android:parentActivityName="akov.example.greattrack.MainActivity"/>
        <activity android:name="akov.example.greattrack.mood.moodStats"/>
        <activity android:name="akov.example.greattrack.mood.allMoodsDisplay"
            android:parentActivityName="akov.example.greattrack.mood.moodStats"/>
    </application>

</manifest>